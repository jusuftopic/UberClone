<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#433F3F"

    tools:context=".Registration.DriverCarDetails.CarInformations.UberBlackDetails">


    <TextView
        android:id="@+id/textView11"
        android:layout_width="132dp"
        android:layout_height="29dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="268dp"
        android:gravity="center"
        android:text="Enterior  color"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carname_uberBlack" />

    <Spinner
        android:id="@+id/enteriorpickerUberBlack"
        android:layout_width="96dp"
        android:layout_height="22dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="38dp"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/interiorpickerUberBlack" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="138dp"
        android:layout_height="41dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="200dp"
        android:gravity="center"
        android:text="Interior color"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carname_uberBlack" />

    <EditText
        android:id="@+id/carname_uberBlack"
        android:layout_width="221dp"
        android:layout_height="45dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:gravity="center"
        android:hint="Mark of car"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/numberofdoors_uberBlack"
        android:layout_width="75dp"
        android:layout_height="47dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="21dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/carname_uberBlack" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="135dp"
        android:layout_height="45dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="Number of doors"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carname_uberBlack" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="135dp"
        android:layout_height="39dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="Max passangers"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/maxpassangers_uberBlack"
        android:layout_width="74dp"
        android:layout_height="40dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="14dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/numberofdoors_uberBlack" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="64dp"
        android:layout_height="49dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="12dp"
        android:gravity="center"
        android:text="Price/$"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8">

    </TextView>

    <EditText
        android:id="@+id/price_uberBlack"
        android:layout_width="78dp"
        android:layout_height="47dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/maxpassangers_uberBlack"></EditText>

    <Button
        android:id="@+id/submitCarDetails_uberBlack"
        android:layout_width="152dp"
        android:layout_height="44dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="12dp"
        android:backgroundTint="#000000"
        android:onClick="addCarToDatabase"
        android:text="Add car"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/warning_uberBlack" />

    <TextView
        android:id="@+id/warning_uberBlack"
        android:layout_width="349dp"
        android:layout_height="23dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="176dp"
        android:gravity="center"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/price_uberBlack" />

    <Spinner
        android:id="@+id/interiorpickerUberBlack"
        android:layout_width="96dp"
        android:layout_height="22dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="35dp"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/price_uberBlack" />

    <CheckBox
        android:id="@+id/airport_permit"
        android:layout_width="137dp"
        android:layout_height="33dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="14dp"
        android:text="Airport permit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/enteriorpickerUberBlack" />

</androidx.constraintlayout.widget.ConstraintLayout>