<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#433F3F"
    tools:context=".Registration.DriverCarDetails.CarInformations.UberSUVDetails">


    <TextView
        android:id="@+id/textView11"
        android:layout_width="135dp"
        android:layout_height="45dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="260dp"
        android:gravity="center"
        android:text="Enterior  color"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carname_uberSUV" />

    <Spinner
        android:id="@+id/enteriorpicker"
        android:layout_width="96dp"
        android:layout_height="22dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="38dp"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/interiorpicker" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="135dp"
        android:layout_height="45dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="200dp"
        android:gravity="center"
        android:text="Interior color"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carname_uberSUV" />

    <EditText
        android:id="@+id/carname_uberSUV"
        android:layout_width="221dp"
        android:layout_height="45dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:gravity="center"
        android:hint="Mark of car"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/numberofdoors_uberSUV"
        android:layout_width="75dp"
        android:layout_height="47dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="21dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/carname_uberSUV" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="135dp"
        android:layout_height="45dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="Number of doors"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carname_uberSUV" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="135dp"
        android:layout_height="39dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="Max passangers"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/maxpassangers_uberSUV"
        android:layout_width="74dp"
        android:layout_height="40dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="14dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/numberofdoors_uberSUV" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="64dp"
        android:layout_height="49dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="12dp"
        android:gravity="center"
        android:text="Price/$"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8">

    </TextView>

    <EditText
        android:id="@+id/price_uberSUV"
        android:layout_width="78dp"
        android:layout_height="47dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/maxpassangers_uberSUV"></EditText>

    <Button
        android:id="@+id/submitCarDetails_uberSUV"
        android:layout_width="152dp"
        android:layout_height="44dp"
        android:layout_marginStart="136dp"
        android:layout_marginTop="12dp"
        android:backgroundTint="#000000"
        android:onClick="addCarToDatabase"
        android:text="Add car"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/warning_uberSUV" />

    <TextView
        android:id="@+id/warning_uberSUV"
        android:layout_width="349dp"
        android:layout_height="23dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="136dp"
        android:gravity="center"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/price_uberSUV" />

    <Spinner
        android:id="@+id/interiorpicker"
        android:layout_width="96dp"
        android:layout_height="22dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="35dp"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/price_uberSUV" />


</androidx.constraintlayout.widget.ConstraintLayout>